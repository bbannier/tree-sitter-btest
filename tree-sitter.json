{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "btest",
      "camelcase": "Btest",
      "title": "BTest",
      "scope": "source.btest",
      "file-types": ["btest"],
      "injections": "queries/injections.scm",
      "highlights": "queries/highlights.scm",
      "injection-regex": "btest",
      "class-name": "TreeSitterBtest"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Btest grammar for tree-sitter",
    "authors": [
      {
        "name": "Benjamin Bannier",
        "email": "bbannier@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-btest"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
